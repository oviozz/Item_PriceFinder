import price_grabber

class PriceAnalyzer(price_grabber.PriceGrabber):

    def price_analyze(self, key, value):
        if key != 'Not In Stock' and int(float(key.replace(',',''))) <= int(max(self._price_range)):
            print(f'{value} --------- Price: ${key}')

    def no_stock_item(self, key,value):
        if key == 'Not In Stock':
            print(f'{value} --------- Price: {key}')
